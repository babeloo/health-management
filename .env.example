# 数据库配置
DB_PASSWORD=admin123
REDIS_PASSWORD=redis123
INFLUX_PASSWORD=influx123
INFLUX_TOKEN=my-super-secret-auth-token
MONGO_PASSWORD=mongo123
EMQX_PASSWORD=emqx123
EMQX_HOST=emqx
MINIO_USER=admin
MINIO_PASSWORD=minio123

# 应用配置
NODE_ENV=development
PORT=5000
CORS_ORIGINS=http://localhost:3000,http://localhost:3001

# JWT配置
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_REFRESH_SECRET=your-super-secret-refresh-jwt-key-change-this-in-production
JWT_EXPIRES_IN=24h
JWT_REFRESH_EXPIRES_IN=7d

# AI服务配置
DEEPSEEK_API_KEY=your-deepseek-api-key-here
DEEPSEEK_BASE_URL=https://api.deepseek.com

# Embedding API 配置
# provider 可选: openai (使用 OpenAI API) 或 local (本地模型)
EMBEDDING_PROVIDER=openai
# OpenAI API Key (如使用 openai provider,未设置则回退到 DEEPSEEK_API_KEY)
EMBEDDING_API_KEY=your-openai-api-key-here
# OpenAI API 地址
EMBEDDING_BASE_URL=https://api.openai.com/v1
# Embedding 模型
EMBEDDING_MODEL=text-embedding-ada-002

# 对象存储配置
OSS_ENDPOINT=http://localhost:9000
OSS_BUCKET=health-mgmt
OSS_ACCESS_KEY=admin
OSS_SECRET_KEY=minio123
